#include <stdio.h>
int main() {
	
	int matriz[11][11], i, j, soma = 0;
	
	for(i=0; i<11; i++){
		for(j=0; j<11; j++){
			//printf("Digite um valor: ");
			//scanf("%d", &matriz[i][j]);
			matriz[i][j] = 1;
			
			if(i == j)
				printf("%d\n", matriz[i][j]);
				else if (j > i)
					soma = soma + matriz[i][j];
		} 
	}
	
	printf("soma = %d", soma);
	
	return 0;
}
